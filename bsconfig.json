{
  "name": "mvp-station-work",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "reason": {
    "react-jsx": 3
  },
  "refmt": 3,
  "suffix": ".mjs",
  "bs-dependencies": [
    "rescript-js",
    "rescript-nodejs",
    "@rescript/react",
    "@rescriptbr/ancestor",
    "@rescriptbr/radix-ui",
    "@greenlabs/rescript-date-fns",
    "rescript-next"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-ts/ppx"
  ],
  "bsc-flags": ["-open ReScriptJs.Js", "-bs-super-errors"]
}
